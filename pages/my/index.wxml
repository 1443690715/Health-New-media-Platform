<view class="container">
    <view class="top">
      <view  bindtap="bindViewTap" class="userinfo">
        <image class="userinfo-avatar" src="{{userInfo.avatarUrl}}" background-size="cover"></image>
        <text class="userinfo-nickname">{{userInfo.nickName}}</text>
      </view>
    </view>
    <view class="menu">
      <view class="dingdan" bindtap="personal">
        <image src="../../image/dingdan.png"/>
        <view >修改个人资料</view>
      </view>
      <view class="guanzhu">
        <image src="../../image/guanzhu.png"/>
        <view>家庭</view>
      </view>
    </view>
    <view class="content">
      <view class="lists">
        <view class="list" bindtap="myreport">个人健康报告<view class="next"></view></view>
        <view class="list" bindtap="healthtest">健康自测<view class="next"></view></view>
        <view class="list" bindtap="addfamilymember">添加家庭成员<view class="next"></view></view>
        <view class="list">我的收藏<view class="next"></view></view>
      </view>
    </view>
</view>
